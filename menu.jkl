EXTERN FN GetMenuItems() : ^VOID

#ASM [
GetMenuItems:
.global GetMenuItems
    mov a3, menu_items_root
    ret

menu_items_root:
    .db 1
    .dp menu_items_file_list
    .dp menu_items_file_name
menu_items_file_name:
    .db 4 .ds "File" .db 0
menu_items_file_list:
    .db 1
    .db 6
    .db 4 .ds "Save" .db 0
]
